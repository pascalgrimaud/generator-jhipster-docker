{
  "name": "generator-jhipster-docker",
  "version": "2.4.0",
  "description": "Additional Docker support: Docker Hub, Local SMTP Server, NGinx",
  "keywords": [
    "yeoman-generator",
    "jhipster-module",
    "docker",
    "docker-compose"
  ],
  "homepage": "https://github.com/pascalgrimaud/generator-jhipster-docker",
  "author": {
    "name": "Pascal Grimaud",
    "email": "",
    "url": "https://twitter.com/pascalgrimaud"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pascalgrimaud/generator-jhipster-docker.git"
  },
  "dependencies": {
    "chalk": "2.3.0",
    "generator-jhipster": ">=4.6.0",
    "mkdirp": "0.5.1",
    "remote-origin-url": "1.0.0",
    "yeoman-generator": "2.0.2"
  },
  "devDependencies": {
    "eslint": "4.7.2",
    "eslint-config-airbnb-base": "12.0.0",
    "eslint-plugin-import": "2.7.0",
    "fs-extra": "4.0.2",
    "mocha": "3.5.3",
    "yeoman-assert": "3.1.0",
    "yeoman-test": "1.7.0"
  },
  "scripts": {
    "pretest": "eslint .",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
